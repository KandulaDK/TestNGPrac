<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OrangeSuite" parallel="tests" thread-count="3">
	<test thread-count="5" name="OrangeHRMLaunch" enabled="true">
		<classes>
			<class name="TestNGBasic.class1">
				<methods>
					<include name="LaunchApp" />
					<include name="EnterLoginDetails" />
					<include name="VerifyLogin" />
				</methods>
			</class>
		</classes>
	</test>
	<test thread-count="5" name="OrangeHRMMyInfo" enabled="true">
		<classes>
			<class name="TestNGBasic.class1">
				<methods>
					<include name="LaunchApp" />
					<include name="EnterLoginDetails" />
					<include name="NavigateToMyInfo" />
					<include name="VerifyMyInfo" />
				</methods>
			</class>
		</classes>
	</test>

	<test thread-count="5" name="Google">
		<classes>
			<class name="TestNGBasic.google">
				<methods>
					<include name="openGoogle" />
					<include name="enterTheSearch" />
				</methods>
			</class>
		</classes>
	</test>

</suite> 
